<project><!-- CACHE ID 239b8550-a5ff-44be-a124-5e7ecd70737d -->
  <import file="sys.xml"/>
  <import file="scheduler.xml"/>
  <import file="rlog.xml"/>
  <import file="vdl.k"/>
  <types>
     <xs:schema targetNamespace="http://ci.uchicago.edu/swift/2009/02/swiftscript" xmlns="http://ci.uchicago.edu/swift/2009/02/swiftscript" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
      	   <xs:simpleType name="messagefile">
      	   <xs:restriction base="string"/>
      	   </xs:simpleType>  
     </xs:schema>	 
  </types>
  <global name="swift#string#17002">
    <vdl:new type="string" value=".result" />
  </global>
  <global name="swift#string#17000">
    <vdl:new type="string" value="a1sm1ms b1sm1ms c1sm1ms" />
  </global>
  <element name="countwords" arguments="t,f">
    <parameterlog>
    <string>input</string>
    <string>f</string>
    <vdl:getdatasetprovenanceid var="{f}" />
    <string>{#thread}</string>
    </parameterlog>
    <parameterlog>
    <string>output</string>
    <string>t</string>
    <vdl:getdatasetprovenanceid var="{t}" />
    <string>{#thread}</string>
    </parameterlog>
    <log level="debug" message="PROCEDURE line=4 thread={#thread} name=countwords"/>
    <vdl:execute>
      <vdl:tr>sh</vdl:tr>
      <vdl:stagein var="{f}"/>
      <vdl:stageout var="{t}"/>
      <vdl:arguments>
        <swiftscript:filename>
         <variable>f</variable> 
        </swiftscript:filename>
      </vdl:arguments>
      <vdl:stdout>
        <swiftscript:filename>
         <variable>t</variable> 
        </swiftscript:filename>
      </vdl:stdout>
    </vdl:execute>
    <vdl:closedataset var="{t}"/>
    <log level="debug" message="PROCEDURE_END line=4"/></element>

  <set name="outputNames">
        <vdl:new type="string" dbgname="outputNames" waitfor="88000" />
  </set>
  <parameterlog>
  <string>intermediate</string>
  <string>outputNames</string>
  <vdl:getdatasetprovenanceid var="{outputNames}" />
  <string>{#thread}</string>
  </parameterlog>
  <set name="outputfiles">
    <vdl:new type="messagefile[]" dbgname="outputfiles">
      <vdl:mapping descriptor="fixed_array_mapper">
        <vdl:parameter name="files"><variable>outputNames</variable></vdl:parameter>
        <vdl:parameter name="input" value="true" />
      </vdl:mapping>
    </vdl:new>
  </set>
  <parameterlog>
  <string>intermediate</string>
  <string>outputfiles</string>
  <vdl:getdatasetprovenanceid var="{outputfiles}" />
  <string>{#thread}</string>
  </parameterlog>
  <restartLog>
  	<vdl:mains>
		<vdl:startprogressticker />
		<vdl:mainp>
		    <parallel>
		        <sequential>
		             <vdl:setfieldvalue>
		               <argument name="var">
		                 <variable>outputNames</variable>
		               </argument>
		               <argument name="value">
		                 <variable>swift#string#17000</variable>
		               </argument>
		             </vdl:setfieldvalue>
		            <partialCloseDataset var="{outputNames}" closeID="88000" />
		        </sequential>
		        <sequential>
		          <vdl:tparallelFor name="$">
		            <getarrayiterator><variable>outputfiles</variable></getarrayiterator>
		            <set names="$$, f">
		              <each items="{$}"/>
		            </set>
		              <set name="i">
		                <vdl:new type="int" value="{$$}"/>
		              </set>	<log level="debug" message="FOREACH_IT_START line=12 thread={#thread}"/>
		          <log level="debug"><string>SCOPE thread={#thread}</string></log>

		              <set name="swift#mapper#17001">
		                    <vdl:new type="string" dbgname="swift#mapper#17001" waitfor="" />
		              </set>
		              <parameterlog>
		              <string>intermediate</string>
		              <string>swift#mapper#17001</string>
		              <vdl:getdatasetprovenanceid var="{swift#mapper#17001}" />
		              <string>{#thread}</string>
		              </parameterlog>
		              <set name="c">
		                <vdl:new type="messagefile" dbgname="c" waitfor="88001">
		                  <vdl:mapping descriptor="single_file_mapper">
		                    <vdl:parameter name="file"><variable>swift#mapper#17001</variable></vdl:parameter>
		                  </vdl:mapping>
		                </vdl:new>
		              </set>
		              <parameterlog>
		              <string>intermediate</string>
		              <string>c</string>
		              <vdl:getdatasetprovenanceid var="{c}" />
		              <string>{#thread}</string>
		              </parameterlog>
		                <parallel>
		                    <sequential>
		                         <vdl:setfieldvalue>
		                           <argument name="var">
		                             <variable>swift#mapper#17001</variable>
		                           </argument>
		                           <argument name="value">
		                             <swiftscript:strcat>
		                              <variable>i</variable><variable>swift#string#17002</variable> 
		                             </swiftscript:strcat>
		                           </argument>
		                         </vdl:setfieldvalue>
		                    </sequential>
		                    <sequential>
		                      <countwords>
		                        <parallel>
		                          <variable>c</variable>
		                          <variable>f</variable>
		                        </parallel>
		                      </countwords>
		                        <partialCloseDataset var="{c}" closeID="88001" />
		                    </sequential>
		                </parallel>
		              <log level="debug" message="FOREACH_IT_END line=12 thread={#thread}"/>
		          </vdl:tparallelFor>
		        </sequential>
		    </parallel>
		</vdl:mainp>
		<vdl:stopprogressticker />
	</vdl:mains>
  </restartLog>  
</project>
